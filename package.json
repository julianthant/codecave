{
  "name": "codecave",
  "version": "0.1.0",
  "private": true,
  "description": "CodeCave.tech - A comprehensive platform for developers",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "clean": "turbo clean",
    "type-check": "turbo type-check",
    "web:dev": "pnpm --filter @codecave/web dev",
    "api:dev": "pnpm --filter @codecave/api dev",
    "test:e2e": "pnpm --filter @codecave/web test"
  },
  "devDependencies": {
    "@types/node": "^20",
    "prettier": "^3.0.0",
    "turbo": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "better-auth": "^1.3.2",
    "require-in-the-middle": "^7.5.2"
  }
}
