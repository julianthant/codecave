#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/julianhein/Work/Personal Projects/codecave/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_reflect-metadata@0.1.14_ts-node@10.9.2/node_modules/typeorm/node_modules:/Users/julianhein/Work/Personal Projects/codecave/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_reflect-metadata@0.1.14_ts-node@10.9.2/node_modules:/Users/julianhein/Work/Personal Projects/codecave/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/julianhein/Work/Personal Projects/codecave/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_reflect-metadata@0.1.14_ts-node@10.9.2/node_modules/typeorm/node_modules:/Users/julianhein/Work/Personal Projects/codecave/node_modules/.pnpm/typeorm@0.3.25_pg@8.16.3_reflect-metadata@0.1.14_ts-node@10.9.2/node_modules:/Users/julianhein/Work/Personal Projects/codecave/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
